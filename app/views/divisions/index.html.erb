<ul class="breadcrumb">
  <li>
    <%= link_to "Divisions", divisions_path %> <span class="divider">|</span>
  </li>
</ul>

<p id="notice"><%= notice %></p>

<table class="table">
  <tr>
    <th>Name</th>
    <th>League</th>
    <th></th>
    <th></th>
  </tr>

<% @divisions.each do |division| %>
  <tr>
    <td><%= link_to division.name, division %></td>
    <td><%= division.league.name %></td>
    <td><%= link_to 'Edit', edit_division_path(division) %></td>
    <td><%= link_to 'Destroy', division, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Division', new_division_path %>
